/******************************************************************************
POSE LIBRARY STYLES.
******************************************************************************/
CaptureWidget
{
    background-color: #FF0000;
    border-bottom: 1px solid rgb(203, 203, 203);
}

GridEntry
{
    outline: none;
}

GridEntry:hover
{
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
				stop: 0.0 rgb(94, 94, 94), 
				stop: 1.0 rgb(64, 64, 64));
}

